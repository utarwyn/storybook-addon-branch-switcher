{
  "name": "storybook-branch-switcher",
  "version": "0.3.0",
  "description": "A Storybook addon to navigate between multiple Git branches.",
  "keywords": [
    "storybook-addons",
    "organize",
    "storybook",
    "add-on",
    "git",
    "branches",
    "git-branches"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utarwyn/storybook-branch-switcher.git"
  },
  "license": "MIT",
  "author": "utarwyn <maxime.malgorn@laposte.net>",
  "bin": {
    "sb-branch-switcher": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./manager": "./dist/manager.mjs",
    "./preview": "./dist/preview.mjs",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "start": "run-p build:watch \"storybook --quiet\"",
    "cli": "npm run build && node ./dist/cli.mjs",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "npm run build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.7",
    "@storybook/addon-interactions": "7.6.7",
    "@storybook/addon-links": "7.6.7",
    "@storybook/blocks": "7.6.7",
    "@storybook/components": "7.6.7",
    "@storybook/core-events": "7.6.7",
    "@storybook/icons": "1.2.3",
    "@storybook/manager": "7.6.7",
    "@storybook/manager-api": "7.6.7",
    "@storybook/preview": "7.6.7",
    "@storybook/preview-api": "7.6.7",
    "@storybook/react": "7.6.7",
    "@storybook/react-vite": "7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "7.6.7",
    "@storybook/types": "7.6.7",
    "@types/node": "^18.19.4",
    "@types/react": "^16.14.55",
    "@types/react-dom": "^16.9.24",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.3",
    "auto": "^11.0.4",
    "boxen": "^7.1.1",
    "dedent": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prompts": "^2.4.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^5.0.5",
    "storybook": "7.6.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.1",
    "vitest": "^1.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "nodeEntries": [
      "src/cli.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Branch switcher",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://raw.githubusercontent.com/utarwyn/storybook-branch-switcher/main/docs/icon.png"
  }
}
